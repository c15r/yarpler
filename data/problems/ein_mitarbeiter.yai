model {
      class Tag {
      }

      class Diensttyp {
        var integer anzahl;
      }

      class Mitarbeiter {
      }

      class Mitarbeitertag {
          var Diensttyp dienst;
          const Mitarbeiter mitarbeiter;
          const Tag tag;
      }
}

initial {
  # instanzen erstellen
  t1 = new Tag();
  d1 = new Diensttyp(anzahl=1);
  f = new Diensttyp();
  m1 = new Mitarbeiter();
  m1t1 = new Mitarbeitertag();

  relation(m1t1.mitarbeiter, m1);
  relation(m1t1.tag, t1);
}

solve {
  # DienstschlÃ¼ssel einhalten
  constraint {
    count(d in Mitarbeitertag.dienst where d==d1) == d1.anzahl
  }

  satisfy;
}