model {
    class Woche {
        const integer anzahl;
        const collection Dienst dienste;
    }

    class Dienst {
        var integer b;
    }
}

initial {
    woche = new Woche(anzahl=1);

    d1 = new Dienst(b=0..1);
    d2 = new Dienst(b=0..1);

    relation(woche.dienste, {d1,d2});
}

solve {
    constraint
        countall(d from Dienst) { d.b == 1 } == woche.anzahl;

    satisfy;
}