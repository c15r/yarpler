domain {
      class Mitarbeiter {
        relation(Tag) tage {
         Tag.dienst;
        }
      }

      class Dienst {
        const integer anzahl;
      }

      class Tag {
        var Dienst dienst;
      }
}

initial {
  d1 = new Dienst(anzahl = 3);
  d2 = new Dienst(anzahl = 7);
  f = new Dienst(anzahl = 0);
  t0 = new Tag(dienst=[d1,d2,f]);
  t1 = new Tag(dienst=[d1,d2,f]);
  t2 = new Tag(dienst=[d1,d2,f]);
  t3 = new Tag(dienst=[d1,d2,f]);
  t4 = new Tag(dienst=[d1,d2,f]);
}