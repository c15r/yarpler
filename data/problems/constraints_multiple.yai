model {
      class Dienst {
        const integer anzahl;
      }
}

initial {
  d1 = new Dienst(anzahl = 1);
  d2 = new Dienst(anzahl = 1);
  d3 = new Dienst(anzahl = 3);
  d4 = new Dienst(anzahl = 3);
}


solve {
    constraint {
      d1.anzahl == d2.anzahl
    }

    constraint {
        d3.anzahl == d4.anzahl
    }

    satisfy;
}